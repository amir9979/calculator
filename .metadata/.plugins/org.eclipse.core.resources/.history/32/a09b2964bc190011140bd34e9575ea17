package dev.log.trace;

import java.util.Map;

public class SpectrumBasedFaultLocalization {

	private static final String DATABASE_NAME = "service_logging";
	private static final String TABLE_NAME = "trace_point";
	private static final String ERROR_TABLE_NAME = "trace_point_error";
	
	public static float sJ = 0;
	public static float sT = 0;
	public static float sO = 0;
	
	private static MySQLConnector connector = new MySQLConnector(DATABASE_NAME);
	
	Map<Map<String,Boolean>> hitMap = new Map<Map<String,Boolean>>();
	
	public static void addError(String process_id) {
		connector.update(String.format("INSERT INTO %s VALUES (0, '%s');",ERROR_TABLE_NAME,process_id));
	}
	
	public static void createMetrics() {
		connector.select("SELECT FROM )
	}
}
