import org.ebayopensource.turmeric.runtime.common.pipeline.InboundMessage;
import org.ebayopensource.turmeric.runtime.common.pipeline.MessageContext;

public class ResponseLogHeaderHandler extends BaseHandler{

	@Override
	public void invoke(MessageContext ctx) throws ServiceException {

		if(ctx.getRequestMessage() instanceof InboundMessage) {
			
		} else {
			System.out.println(ctx.getResponseMessage().getTransportHeader(RequestLogHeaderHandler.PROCESS_ID));
		}
		
	}

}
