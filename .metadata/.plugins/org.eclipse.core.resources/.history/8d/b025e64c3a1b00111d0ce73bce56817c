package dev.log.trace.test;

import java.util.ArrayList;

import dev.log.trace.SpectrumBasedMultipleFaultLocalization;

public class TestSpectrumBasedMultipleFaultLocalization {

	public static void main(String[] args) {
		ArrayList<ArrayList<Integer>> matrix = new ArrayList<ArrayList<Integer>>();
		ArrayList<Integer> c1 = new ArrayList<Integer>();
		//c1.add(1);c1.add(0);c1.add(1);
		c1.add(1);c1.add(1);c1.add(1);c1.add(1);c1.add(1);c1.add(1);
		
		ArrayList<Integer> c2 = new ArrayList<Integer>();
		//c2.add(0);c2.add(1);c2.add(0);
		c2.add(0);c2.add(1);c2.add(1);c2.add(1);c2.add(1);c2.add(1);
		
		ArrayList<Integer> c3 = new ArrayList<Integer>();
		//c3.add(1);c3.add(1);c3.add(1);
		c3.add(0);c3.add(0);c3.add(1);c3.add(1);c3.add(1);c3.add(1);
		
		ArrayList<Integer> c4 = new ArrayList<Integer>();
		c4.add(0);c4.add(0);c4.add(1);c4.add(1);c4.add(1);c4.add(0);
		
		ArrayList<Integer> c5 = new ArrayList<Integer>();
		c5.add(0);c5.add(0);c5.add(1);c5.add(1);c5.add(1);c5.add(1);
		
		ArrayList<Integer> c6 = new ArrayList<Integer>();
		c6.add(1);c6.add(1);c6.add(1);c6.add(0);c6.add(1);
		
		ArrayList<Integer> e = new ArrayList<Integer>();
		//e.add(1);e.add(1);e.add(0);
		c6.add(1);c6.add(1);c6.add(1);c6.add(0);c6.add(1);
		
		
		matrix.add(c1);matrix.add(c2);matrix.add(c3);matrix.add(c4);matrix.add(c5);matrix.add(c6);
		matrix.add(e);
		
		
		
		SpectrumBasedMultipleFaultLocalization local = new SpectrumBasedMultipleFaultLocalization();
		ArrayList<Double> h = local.calculateSO(matrix);
		System.out.println(h);
	}
}
